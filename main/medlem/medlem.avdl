@namespace("no.nav.aap.avro.medlem.v1")
protocol MedlemV1 {
	enum ErMedlem {JA, NEI, UAVKLART}

	record Request {
		date  mottattDato;
		string ytelse;
		boolean arbeidetUtenlands;
	}

	record Response {
		ErMedlem erMedlem;
		union{null, string} begrunnelse = null;
	}

	record Medlem {
		string personident;
		string id;
		union{null, Request} request = null;
		union{null, Response} response = null;
	}
}
